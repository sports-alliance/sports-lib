{
  "name": "quantified-self-lib",
  "version": "2.2.3",
  "description": "A Library to for processing GPX, TCX, FIT and JSON files from services such as Strava, Movescount, Garmin, Polar etc",
  "keywords": [
    "gpx",
    "tcx",
    "fit",
    "activities",
    "json",
    "suunto",
    "garmin",
    "polar",
    "export",
    "import",
    "reader",
    "library",
    "utility",
    "parser"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jimmykane/quantified-self-lib.git"
  },
  "bugs": {
    "url": "https://github.com/jimmykane/quantified-self-lib/issues"
  },
  "homepage": "https://github.com/jimmykane/quantified-self-lib",
  "author": "Dimitrios Kanellopoulos",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@types/node": "^10.12.12",
    "easy-fit": "github:jimmykane/easy-fit",
    "fast-xml-parser": "^3.12.12",
    "geolib": "^2.0.24",
    "gxparser": "^1.0.0",
    "lowpassf": "^0.5.0",
    "moving-median": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "jest": "^23.6.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  }
}
