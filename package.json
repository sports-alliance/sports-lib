{
  "name": "quantified-self-lib",
  "version": "1.0.0-pre-5",
  "description": "A utility lib to work with tcx, fit and Suunto files",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jimmykane/quantified-self-lib.git"
  },
  "bugs": {
    "url": "https://github.com/jimmykane/quantified-self-lib/issues"
  },
  "homepage": "https://github.com/jimmykane/quantified-self-lib",
  "author": "Dimitrios Kanellopoulos",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@types/uuid": "^3.4.3",
    "easy-fit": "github:jimmykane/easy-fit",
    "geolib": "^2.0.24",
    "lowpassf": "^0.5.0",
    "moving-median": "^1.0.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  }
}
